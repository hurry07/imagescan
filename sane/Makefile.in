	    echo "#  Copyright (C) 2007  EPSON AVASYS CORPORATION" >> $@;\
	    echo "#" >> $@; \
	    echo "#  License: GPL-2.0+ (same as ltmain.sh)" >> $@; \
	    echo "#  Author : EPSON AVASYS CORPORATION" >> $@; \
	    echo "#" >> $@; \
