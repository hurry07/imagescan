install-exec_sane_backendLTLIBRARIES: $(exec_sane_backend_LTLIBRARIES)
	@$(NORMAL_INSTALL)
	test -z "$(exec_sane_backenddir)" || $(MKDIR_P) "$(DESTDIR)$(exec_sane_backenddir)"
	@list='$(exec_sane_backend_LTLIBRARIES)'; test -n "$(exec_sane_backenddir)" || list=; \
	list2=; for p in $$list; do \
	  if test -f $$p; then \
	    list2="$$list2 $$p"; \
	  else :; fi; \
	done; \
	test -z "$$list2" || { \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 '$(DESTDIR)$(exec_sane_backenddir)'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 "$(DESTDIR)$(exec_sane_backenddir)"; \
	}

uninstall-exec_sane_backendLTLIBRARIES:
	@$(NORMAL_UNINSTALL)
	@list='$(exec_sane_backend_LTLIBRARIES)'; test -n "$(exec_sane_backenddir)" || list=; \
	for p in $$list; do \
	  $(am__strip_dir) \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f '$(DESTDIR)$(exec_sane_backenddir)/$$f'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f "$(DESTDIR)$(exec_sane_backenddir)/$$f"; \
	done

clean-exec_sane_backendLTLIBRARIES:
	-test -z "$(exec_sane_backend_LTLIBRARIES)" || rm -f $(exec_sane_backend_LTLIBRARIES)
	@list='$(exec_sane_backend_LTLIBRARIES)'; for p in $$list; do \
	  dir="`echo $$p | sed -e 's|/[^/]*$$||'`"; \
	  test "$$dir" != "$$p" || dir=.; \
	  echo "rm -f \"$${dir}/so_locations\""; \
	  rm -f "$${dir}/so_locations"; \
	done
